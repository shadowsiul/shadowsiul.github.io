.m-page-bar { width: 100%; max-width: calc(10% + 1600px); padding: 0px 5%; =
margin: 0px auto; display: flex; position: relative; left: 0px; right: 0px;=
 background: rgb(255, 255, 255); border-bottom: 1px solid rgb(230, 230, 230=
); }

.m-page-bar.f-hidden { display: none; }

.m-page-bar.f-sticky { z-index: 500; visibility: hidden; position: fixed; t=
op: -60px; transition: 0.4s cubic-bezier(0.19, 1, 0.22, 1); max-width: none=
; padding: 7px 5% 4px; }

@media screen and (min-width: 1778px) {
  .m-page-bar.f-sticky { padding: 7px calc(50% - 800px) 4px; }
}

.m-page-bar.f-sticky.f-show { visibility: visible; top: 0px; }

.m-page-bar.f-sticky[itemscope] > div:first-child { overflow: hidden; }

.m-page-bar .c-pivot { width: auto; }

.m-page-bar > div { height: 60px; float: right; flex: 0 1 auto; flex-direct=
ion: row-reverse; }

